{{- $admin_cluster_id := string .Params.cluster -}}
{{- $admin_cluster := index (where (where .Site.Pages "Section" "administrative_clusters") ".Params.cluster" .Params.cluster) 0 -}}
{{- $boe_cluster := index (where (where .Site.Pages "Section" "boe_clusters") ".Params.cluster" .Params.boe_cluster) 0 -}}
{
	"acronym": "{{ .Params.acronym }}",
	"name": "{{ .Params.name }}",
	"primary_color": "{{ .Params.primary_color }}",
	"level": "{{ .Params.level }}",
	"contact": {
		"phone": "{{ .Params.contact.phone }}",
		"fax": "{{ .Params.contact.fax }}"
	},
	"address": {
		"street": "{{ .Params.address.street }}",
		"city": "{{ .Params.address.city }}",
		"postal_code": "{{ .Params.address.postal_code }}",
		"latitude": "{{ .Params.address.latitude }}",
		"longitude": "{{ .Params.address.longitude }}"
	},
	"principal": "{{ .Params.principal }}",
	"hours": {
		"open": {{ if .Params.hours.open }}"{{ .Params.hours.open }}"{{ else }}null{{ end }},
		"close": {{ if .Params.hours.close }}"{{ .Params.hours.close }}"{{ else }}null{{ end }}
	},
	"profile": {{ if .Params.profile }}"{{ .Params.profile }}"{{ else }}null{{ end }},
	"msde_report": {{ if .Params.msde_report }}"{{ .Params.msde_report }}"{{ else }}null{{ end }},
	"walk_area": {{ if .Params.walk_area }}"{{ .Params.walk_area }}"{{ else }}null{{ end }},
	"title_1": {{ .Params.title_1 }},
	"administrative_cluster": {
		"community_superintendent": {
			"name": "{{ $admin_cluster.Params.community_superintendent.name }}",
			"phone": "{{ $admin_cluster.Params.community_superintendent.phone }}",
			"email": "{{ $admin_cluster.Params.community_superintendent.email }}"
		},
		"pec_officer": {
			"name": "{{ $admin_cluster.Params.pec_officer.name }}",
			"phone": "{{ $admin_cluster.Params.pec_officer.phone }}",
			"email": "{{ $admin_cluster.Params.pec_officer.email }}"
		}
	},
	"boe_cluster": {
		"representative": "{{ $boe_cluster.Params.representative }}"
	},
	"achievements": [
	{{- range $index, $award := .Params.achievements }}
		{{ if $index }},{{ end }}{
			"machine_name": "{{ $award.machine_name }}",
			"name": "{{ $award.name }}"
			{{- if $award.level -}},
			"level": "{{ $award.level }}"
			{{- end -}}
			{{- if $award.url -}},
			"url": "{{ $award.url }}"
			{{- end -}}
			{{- if $award.color -}},
			"color": "{{ $award.color }}"
			{{- end -}}
			{{- if $award.years -}},
			"years": [{{ "\"" }}{{ delimit $award.years "\", \"" }}{{ "\"" }}]
			{{- end }}
		}
	{{ end }}
	]
}
