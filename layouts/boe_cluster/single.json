{{- $cluster := index .Site.Data.boe_clusters .Params.cluster -}}
{{- $schools := where (where .Site.Pages "Section" "schools") ".Params.boe_cluster" $cluster.cluster -}}
{
	"cluster": "{{ $cluster.cluster }}",
	"representative": "{{ $cluster.representative }}",
	"schools": [
	{{- range $index, $school := $schools -}}
		{{- if $index -}},{{- end -}}
		"{{ $school.Params.acronym }}"
	{{- end -}}
	]
}
