{{- $cluster := index .Site.Data.boe_clusters .Params.cluster -}}
{{- range $school := .Site.Data.schools -}}
	{{- if eq $school.boe_cluster $cluster.cluster -}}
		{{- $.Scratch.Add "schools" (slice $school.acronym) -}}
	{{- end -}}
{{- end -}}
{
	"cluster": "{{ $cluster.cluster }}",
	"representative": "{{ $cluster.representative }}",
	"schools": [
		"{{ delimit ($.Scratch.Get "schools") "\", \"" }}"
	]
}
